button {
margin-bottom: 0px;
border-radius: 0px;
width: 130px;
height: 30px;
margin-top: 55px;
margin-left: -50px;
color: #FFFFFF;
background-color: #0033FF;
font-family: Verdana;
font-weight: bold;
font-size: 8pt;
padding: 2px 2px 2px 2px;
text-align: center;
border-left: 1px solid #FF4602;
border-bottom: 1px solid #999999;
border-top: 1px;
border-right: 1px solid #FFFFFF;
border-style: none;
color: #FFFFFF;
font-weight: bold;
background: url("/image/btAtivado.png");
background-repeat: no-repeat;
background-position: center;
}

body {
position: all;
top: 0px;
right: 0px;
bottom: 0px;
left: 0px;
width: 100%;
height: 100%;
margin:0;
padding:0;
text-align: left;
overflow-y: scroll;
background: black;
}

#tudo {
position: absolute;
top: 0px;
right: 0px;
bottom: 0px;
left: 10px;
width: 500px;
height: 120px;
margin: 0 auto;
box-shadow: white;
padding: 20px;
box-sizing: border-box;
text-align: left;
display: flex;
background-color: white;
}

#conteudo {
padding: 50px;
margin-left: 10px;
margin-top: -60px;
margin-left: -40px;
background-color: white;
text-align: left;
width: 100%;
height: 75%;
}

#extractor{
margin-top: 140px;
margin-left: 30px;
margin-right: 0px !important;
padding-left: 10px;
padding-right: 10px !important;
width: 94%;
display: inline-block;
white-space: normal;
border-right: 50px;
background: black;
}

.row {
 clear: both;
 }

.column-3 {    
 width: 35%;    
}

.column-9 {    
 width: 100%;    
}

.label {
 padding-top: 7px;
 text-align: right;    
}

.label:after {
 content: ':';
 padding-right: 5px;
}

.column {
 float: left;
 height: 41px;    
}

.textarea textarea {
 width: 200px;
 height: 100px;
 margin-top: 10px;    
 padding-left: 50px;
 box-sizing: border-box;
 border-radius: 20px;
 box-sizing: 100%;
}

p{
	color: black;
	min-height: 500%;
}

#dsHistorico{
	width: 350px;
	height: 85px;
	margin-left: -20px;
}